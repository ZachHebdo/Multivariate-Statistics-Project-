truncate <- function(x, alpha) {
x <- sort(x)
n <- length(x)
# Calculer le nombre de valeurs à tronquer de chaque côté
trunc_amount = ceiling(alpha * n)
# Supprimer les valeurs tronquées des deux extrémités
